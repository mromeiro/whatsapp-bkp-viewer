{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {

          "path": "consoleapp.log",
          "outputTemplate": "[{Timestamp:HH:mm:ss}] [{Level}] [{MemberName}:{LineNumber}] {Message} {Exception}{NewLine}"
        }
      }
    ]
  }
}